<!-- 自分の投稿一覧 -->
<h2>Books</h2>
  <p><%= image_tag @user.get_profile_image(100,100) %></p>
  <p><%= @user.name %></p>
  <table>
   <thead>
     <tr>
       <th>Title</th>
       <th>Body</th>
       <th></th>
       <th></th>
     </tr>
   </thead>
   <% @books.each do |book| %>
   <tbody>
     <tr>
       <td><%= book.title %></td>
       <td><%= book.body %></td>
       <td><%= link_to 'Edit', edit_book_path(book.id) %></td>
       <td><%= link_to 'Destroy', book_path(book.id), method: :delete, "data-confirm" => "Are you sure?" %></td>
     </tr>
   </tbody>
   <% end %>
 </table>

